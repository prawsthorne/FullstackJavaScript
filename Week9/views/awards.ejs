<html lang="en">
    <%- include('./partials/head.ejs') %>
    <body>
        <%- include('./partials/nav.ejs') %>
        <div class="awards content">
            <h2>Films with Awards.</h2>
            <% if (awards.length > 0) { %>
                <% awards.forEach(award => { %>
                    <p class="film"> 
                        <a href="<%= award.film_id %>">
                        <b><%= award.title %></b></a> has 
                        <%= award.count %> awards.
                    </p>
                <% }) %>
            <% } %>
        </div>
    </body>
    <%- include('./partials/foot.ejs') %>
</html>